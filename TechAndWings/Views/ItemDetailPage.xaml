<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="TechAndWings.ItemDetailPage" Title="{Binding Title}">
	<StackLayout Spacing="20" Padding="15">
		<Label Text="{Binding Item.Date}" FontSize="Medium" />
		<Label Text="{Binding Item.Location}" FontSize="Small" />
        <Label Text="{Binding Item.Topics}" FontSize="Small" />
        <Label Text="{Binding Item.People}" FontSize="Small" />
	</StackLayout>
</ContentPage>
